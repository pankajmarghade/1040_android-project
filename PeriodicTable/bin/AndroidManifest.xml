<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.periodictable"
    android:versionCode="1"
    android:versionName="1.0" >

    <shape xmlns:android="http://schemas.android.com/apk/res/android" >
        <solid android:color="#f8f8f8" />

        <corners android:radius="4dp" />

        <padding
            android:bottom="5dp"
            android:left="10dp"
            android:right="10dp"
            android:top="5dp" />

        <stroke
            android:width="1dp"
            android:color="#aeaeae" />
    </shape>

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.periodictable.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.periodictable.HydrogenActivity"
            android:label="@string/title_activity_hydrogen" >
        </activity>
        <activity
            android:name="com.example.periodictable.PeriodicActivity"
            android:label="@string/title_activity_periodic" >
        </activity>
        <activity
            android:name="com.example.periodictable.SBlockActivity"
            android:label="@string/title_activity_sblock" >
        </activity>
        <activity
            android:name="com.example.periodictable.LithiumActivity"
            android:label="@string/title_activity_lithium" >
        </activity>
        <activity
            android:name="com.example.periodictable.DBlockActivity"
            android:label="@string/title_activity_dblock" >
        </activity>
        <activity
            android:name="com.example.periodictable.PBlockActivity"
            android:label="@string/title_activity_pblock" >
        </activity>
        <activity
            android:name="com.example.periodictable.FBlockActivity"
            android:label="@string/title_activity_fblock" >
        </activity>
        <activity
            android:name="com.example.periodictable.PBlock1Activity"
            android:label="@string/title_activity_pblock1" >
        </activity>
        <activity
            android:name="com.example.periodictable.InformationActivity"
            android:label="@string/title_activity_information" >
        </activity>
        <activity
            android:name="com.example.periodictable.QuizActivity"
            android:label="@string/title_activity_quiz" >
        </activity>
        <activity
            android:name="com.example.periodictable.HelpActivity"
            android:label="@string/title_activity_help" >
        </activity>
        <activity
            android:name="com.example.periodictable.Quiz1Activity"
            android:label="@string/title_activity_quiz1" >
        </activity>
        <activity
            android:name="com.example.periodictable.SInfoActivity"
            android:label="@string/title_activity_sinfo" >
        </activity>
        <activity
            android:name="com.example.periodictable.PInfoActivity"
            android:label="@string/title_activity_pinfo" >
        </activity>
        <activity
            android:name="com.example.periodictable.DInfoActivity"
            android:label="@string/title_activity_dinfo" >
        </activity>
        <activity
            android:name="com.example.periodictable.FInfoActivity"
            android:label="@string/title_activity_finfo" >
        </activity>
        <activity
            android:name="com.example.periodictable.Help1Activity"
            android:label="@string/title_activity_help1" >
        </activity>
    </application>

</manifest>